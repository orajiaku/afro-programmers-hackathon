doctype html
html(lang='en')
    head
        meta(charset='UTF-8')
        title Socket.IO whiteboard
        link(rel='stylesheet', href='/stylesheets/style2.css')
        link(rel='stylesheet', href='https://maxcdn.bootstrapcdn.com/bootstrap/4.0.0-alpha.6/css/bootstrap.min.css', integrity='sha384-rwoIResjU2yc3z8GV/NPeZWAv56rSmLldC3R/AZzGRnGxQQKnKkoFVhFQhNUwEyJ', crossorigin='anonymous')

    body
        .commentsView#commentsView
            .cancel
                img.icon.icons8-Cancel(onclick='hideComments()' width='100', height='100', src='data:image/png;base64,')
            .commentsBack
                .commentsContainer
                    ul.media-list
                        li.media
                            .media-left
                                a(href='#')
                                    img.media-object(src='https://tse2.mm.bing.net/th?id=OIP.Cybqo9gwLjeOX9WO8CZgbQDLEy&w=199&h=300&c=8&qlt=90&o=4&pid=1.7', alt='...')
                            .media-body
                                h4.media-heading Media heading
                                |       WHAT IS ALL THIS ABOUT

                .inputContainer
                    input.commentInput
                    button.button
                        | Send
        div.icons
            .comment
                img.icon.icons8-Comments(onclick='showComments()' width='70', height='70', src='data:image/png;base64,')

        canvas.whiteboard
        .colors
            .color.black
            .color.red
            .color.green
            .color.blue
            .color.yellow
        script(src='https://cdnjs.cloudflare.com/ajax/libs/socket.io/2.0.3/socket.io.js')
        script(src='https://code.jquery.com/jquery-3.1.1.slim.min.js', integrity='sha384-A7FZj7v+d/sdmMqp/nOQwliLvUsJfDHW+k9Omg/a/EheAdgtzNs3hpfag6Ed950n', crossorigin='anonymous')
        script(src='https://cdnjs.cloudflare.com/ajax/libs/tether/1.4.0/js/tether.min.js', integrity='sha384-DztdAPBWPRXSA/3eYEEUWrWCy7G5KFbe8fFjk5JAIxUYHKkDx6Qin1DkWx51bBrb', crossorigin='anonymous')
        script(src='https://maxcdn.bootstrapcdn.com/bootstrap/4.0.0-alpha.6/js/bootstrap.min.js', integrity='sha384-vBWWzlZJ8ea9aCX4pEW3rVHjgjt7zpkNpZk+02D9phzyeVkE+jo0ieGizqPLForn', crossorigin='anonymous')
        script(src='/javascripts/main.js')
        script(src='/javascripts/custom.js')
